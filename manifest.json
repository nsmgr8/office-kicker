{
    "name": "Office 365 Kicker",
    "version": "0.1",
    "description": "Give Office 365 a damn good thrashing",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [{
        "js": ["office-kicker.js"],
        "matches": [ "http://*/*", "https://*/*"]
    }],
    "browser_action": {
            "default_icon": "icon.png"
    },
    "permissions": [
        "tabs"
    ],
    "manifest_version": 2
}
